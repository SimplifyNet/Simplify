<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net462</TargetFrameworks>
		<Authors>Alexander Krylkov</Authors>
		<Product>Simplify</Product>
		<Description>Simple logger</Description>
		<Copyright>Licensed under LGPL</Copyright>
		<Version>1.5</Version>
		<PackageProjectUrl>https://github.com/SimplifyNet/Simplify/wiki/Simplify.Log</PackageProjectUrl>
		<PackageIconUrl>https://raw.githubusercontent.com/SimplifyNet/Images/master/Logo.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/SimplifyNet/Simplify/tree/master/src/Simplify.Log</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<PackageTags>.NET logger</PackageTags>
		<PackageReleaseNotes>
			Updates
			* Upgrade to Microsoft.Extensions.Configuration 3.0.0
		</PackageReleaseNotes>
		<OutputPath>bin\Any CPU\$(Configuration)\</OutputPath>
		<DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\Simplify.Log.xml</DocumentationFile>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Configuration" Version="3.0.0" />
		<PackageReference Include="System.IO.Abstractions" Version="9.0.4" />
		<Reference Include="System.Configuration" />
		<Reference Include="System.ServiceModel" />
		<Reference Include="System.Web" />
	</ItemGroup>
</Project>