<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;net462</TargetFrameworks>
		<Authors>Alexander Krylkov</Authors>
		<Product>Simplify</Product>
		<Description>Simplify.Repository FluentNHibernate implementation</Description>
		<Copyright>Licensed under LGPL</Copyright>
		<Version>1.2</Version>
		<PackageProjectUrl>https://github.com/SimplifyNet/Simplify</PackageProjectUrl>
		<PackageIconUrl>https://raw.githubusercontent.com/SimplifyNet/Images/master/Logo.png</PackageIconUrl>
		<RepositoryUrl>https://github.com/SimplifyNet/Simplify/tree/master/src/Simplify.Repository.FluentNHibernate</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<PackageTags>.NET Repository UoW Model</PackageTags>
		<PackageReleaseNotes>
			Upgrades according to Simplify.Repository 1.3.0
			* Unit of work async Commit/Rollback methods
			Other
			* Obsolete methods removed
		</PackageReleaseNotes>
		<OutputPath>bin\Any CPU\$(Configuration)\</OutputPath>
		<DocumentationFile>bin\Any CPU\$(Configuration)\$(TargetFramework)\Simplify.Repository.FluentNHibernate.xml</DocumentationFile>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Simplify.FluentNHibernate" Version="2.0.0" />
		<PackageReference Include="Simplify.Repository" Version="1.3.0" />
	</ItemGroup>
</Project>